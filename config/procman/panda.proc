//Default arguments, others will inherit there
:process : object {
    auto-restart = "true";
    restart-delay-ms = "500";
    auto-start = "true";
    host = "panda";
 }

proc# : process {
    cmd = "java botlab.PathFollower";
    name = "Path Follower";
}


proc# : process {
    cmd = "java botlab.PoseGenerator";
    name = "Pose Generator";
}

